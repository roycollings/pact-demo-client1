{
  "consumer": {
    "name": "pact-demo-client1"
  },
  "interactions": [
    {
      "description": "a request for an item",
      "providerState": "Getting an item",
      "request": {
        "matchingRules": {
          "$.path": {
            "match": "regex",
            "regex": "/items/[0-9]"
          }
        },
        "method": "GET",
        "path": "/items/1"
      },
      "response": {
        "body": "{\"item_id\":{\"value\":1,\"pact:matcher:type\":\"type\"}}",
        "headers": {
          "Content-Type": "application/x-www-form-urlencoded"
        },
        "status": 200
      }
    },
    {
      "description": "a request to say hello",
      "providerState": "Saying hello",
      "request": {
        "method": "GET",
        "path": "/"
      },
      "response": {
        "body": {},
        "headers": {
          "Content-Type": "application/json"
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "11.0.2"
    },
    "pactRust": {
      "ffi": "0.4.0",
      "models": "1.0.4"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "pact-demo-api1"
  }
}